{
    "manifest_version": 3,
    "name": "HAR File Recorder V2",
    "version": "1.0",
    "permissions": [
      "debugger",
      "activeTab",
      "scripting",
      "downloads"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  